{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn run api-typings:generate && concurrently \"yarn run api-typings:watch\" \"vite\"",
    "preview": "yarn run build && vite preview --port 3001",
    "build": "yarn run api-typings:generate && tsc && vite build",
    "lint": "yarn run eslint && stylelint && prettier",
    "prettier:fix": "prettier --write .",
    "eslint": "eslint -c .eslintrc.cjs --cache --ext .ts,.tsx .",
    "stylelint": "stylelint ./src/**/*.{ts,tsx}",
    "api-typings:watch": "nodemon --watch src/app/api/schema --ext json --verbose --exec yarn run dts-generate:backend",
    "api-typings:generate": "yarn run clean:api-typings && yarn run dts-generate:backend",
    "dts-generate:backend": "dtsgen --out src/app/api/@generated/backend-models.d.ts src/app/api/schema/backend-openapi.json -c src/app/api/config/dtsgenerator-config-backend.json",
    "clean:api-typings": "rimraf src/app/api/@generated"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "antd": "^5.7.3",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "styled-components": "5.3.11"
  },
  "devDependencies": {
    "@dtsgenerator/replace-namespace": "^1.5.4",
    "@dtsgenerator/single-quote": "^1.6.3",
    "@rollup/plugin-alias": "^5.0.0",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.4.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "concurrently": "^8.2.0",
    "dtsgenerator": "^3.18.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-immer-reducer": "^0.1.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.4.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-redux": "^4.0.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-unicorn": "^48.0.0",
    "lodash": "^4.17.21",
    "meow": "^9.0.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.27",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^3.0.0",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "overrides": {
    "@babel/code-frame": "7.12.11"
  }
}
